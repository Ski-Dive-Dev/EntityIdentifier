    <Description>A project to abstract entity ID's so that their underlying type (e.g., string, int, GUID) can be changed at any time with minimal code changes.</Description>
    <Copyright>(c) 2021</Copyright>
    <PackageProjectUrl>https://github.com/Ski-Dive-Dev/EntityIdentifier</PackageProjectUrl>
    <Authors>Ski-Dive-Dev</Authors>
    <Company>Ski-Dive-Dev</Company>
    <PackageId>EntityIdentifier</PackageId>
    <Product>EntityIdentifier</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="5.0.1" />
  </ItemGroup>

</Project>
